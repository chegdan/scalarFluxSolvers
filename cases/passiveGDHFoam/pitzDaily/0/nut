FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "1010";
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField uniform 2e-5;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 2e-05;
    }
    outlet
    {
        type            fixedValue;
        value           uniform 0;
    }
    upperWall
    {
        type            fixedValue;
        value           uniform 0;
    }
    lowerWall
    {
        type            fixedValue;
        value           uniform 0;
    }
    frontAndBack
    {
        type            empty;
    }
}
